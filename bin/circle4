#!/usr/bin/env raku
my $sol = $*PROGRAM.IO.basename.Str; $sol ~~ s/sol//; say qq:to/HERE/; 
Solution $sol:\n==========
HERE 

class Circle {
    has $.radius;
    has $.diam = $!radius * 2;
    has $.area = pi * $!radius ** 2;
}
my $radius = 2;
my $c = Circle.new: :$radius;
say $c.diam;
say $c.area;


