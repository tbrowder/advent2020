#!/usr/bin/env raku
my $sol = $*PROGRAM.IO.basename.Str; $sol ~~ s/sol//; say qq:to/HERE/; 
Solution $sol:\n==========
HERE 

# sol 1b
class Box {
    has$.llx = 0;
    has$.lly;
    has$.urx;
    has$.ury;
    has$.w;
    has$.h;
    submethod BUILD {
        if $!urx.defined {
            $!w = $!urx - $!llx
        }
        elsif $!w.defined {
            $!w
        }
    }
}
my ($llx, $lly, $w, $h) = 0, 0, 2, 3;
my $o = Box.new: :$llx, :$lly, :$w, :$h;
say $o.w;
say $o.h;

